}
.root {
  position: relative;
  transition: margin 150ms cubic-bezier(0.4, 0, 0.2, 1) 0ms;
  box-sizing: border-box;
}
.root::before {
  position: absolute;
  left: 0;
  bottom: 0;
  right: 0;
  height: 1px;
  content: "";
  opacity: 1;
  background-color: rgba(0, 0, 0, 0.12);
}
.disabled {
  background-color: rgba(0, 0, 0, 0.12);
  color: rgba(0, 0, 0, 0.38);
}
.expanded {
  margin: 32rpx 0;
}
.root-first-child {
  margin-top: 0;
}
.root-last-child {
  margin-bottom: 0;
}
.entered {
  height: auto;
}
.summary {
  display: flex;
  min-height: 96rpx;
  padding: 0 48rpx 0 48rpx;
  position: relative;
}
.summary-content {
  flex-grow: 1;
  display: flex;
  margin: 24rpx 0;
}
.expand-icon {
  position: absolute;
  top: 50%;
  right: 48rpx;
}
.rotate-expand-icon {
  transform: translateY(-50%) rotate(180deg);
}
.detail {
  display: flex;
  padding: 16rpx 48rpx 48rpx;
}
.action {
  padding: 32rpx 32rpx;
}
