.root {
  display: flex;
  justify-content: center;
  align-items: center;
}
.paper {
  display: flex;
  margin: 64rpx;
  flex-direction: column;
  flex: 0 1 auto;
  position: relative;
  max-height: 90vh;
  overflow-y: auto;
  outline: none;
}
.paper-full-width {
  display: flex;
  margin: 64rpx;
  flex-direction: column;
  flex: 0 1 auto;
  position: relative;
  max-height: 90vh;
  overflow-y: auto;
  outline: none;
  width: 100vw;
}
.paper-full-screen {
  display: flex;
  margin: 64rpx;
  flex-direction: column;
  flex: 0 1 auto;
  position: relative;
  max-height: 90vh;
  overflow-y: auto;
  outline: none;
  margin: 0;
  width: 100vw;
  max-width: 100vw;
  height: 100vh;
  max-height: 100vh;
  border-radius: 0;
}
.content {
  visibility: hidden;
  transition: transform 225ms cubic-bezier(0, 0, 0.2, 1) 0ms;
}
.transition-left {
  visibility: visible;
  transform: translateX(-500px);
}
.transition-right {
  visibility: visible;
  transform: translateX(500px);
}
.transition-bottom {
  visibility: visible;
  transform: translateY(500px);
}
.transition-top {
  visibility: visible;
  transform: translateY(-500px);
}
.reset {
  visibility: visible;
  transform: translate(0, 0);
}
